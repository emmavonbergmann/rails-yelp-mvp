<div class="container">
  <h1>Create a new restaurant</h1>
    <%= simple_form_for(@restaurant) do |f| %>
      <%= f.input :name %>
      <%= f.input :address %>
      <%= f.input :phone_number %>
      <%= f.input :category %>
      <%= f.submit "Submit restaurant", class:"btn btn-light"%>
    <% end %>
  <%= link_to "all restaurants", restaurants_path, class:"btn btn-dark" %>
</div>
