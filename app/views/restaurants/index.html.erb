<div class="container">
  <h1>All Restaurants</h1>
    <ul>
      <% @restaurants.each do |restaurant| %>
      <li><%= link_to "#{restaurant.name}", restaurant%></li>
      <% end %>
    </ul>
    <p>
    <%= link_to "create a new restaurant", new_restaurant_path(@restaurant), class:"btn btn-dark"%></p>

</div>
