<div class="container">
  <h1>New Review</h1>
    <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.submit "Submit review", class:"btn btn-dark" %>
    <% end %>
    <%= link_to "all restaurants", restaurants_path, class:"btn btn-dark" %>
</div>
